{
	"parser": "@typescript-eslint/parser",
	"env": {
		"browser": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "prettier"],
	"rules": {
		"@typescript-eslint/ban-ts-comment": ["error", { "ts-ignore": "allow-with-description" }]
	}
}
